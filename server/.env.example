# Example environment variables for the server
PORT=4000

# Secret used to sign JWT tokens. Copy a strong secret into your real .env
JWT_SECRET=change_me_to_a_long_random_string

MONGODB_URI=mongodb://127.0.0.1:27017/automation-tool

# Add other secrets here (don't commit .env with real secrets)
IG_USER_ID=<instagram-user-id>
IG_API_VERSION=v17.0
IG_ACCESS_TOKEN=<long-lived-access-token>